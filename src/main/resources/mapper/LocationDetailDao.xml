<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.space.demo.dao.locationDetail.LocationDetailDao">

    <insert id="batchInsert" parameterType="java.util.List">
        insert into locationDetail(rocketType,carriage,shotTime,workNumber,targetTrack,result,locationName) values
        <foreach collection="list" item="item" separator=",">
            (#{item.rocketType},#{item.carriage},#{item.shotTime},#{item.workNumber},#{item.targetTrack},#{item.result},#{item.locationName})
        </foreach>
    </insert>

    <select id="getAllByName" resultType="com.space.demo.entity.visualData.localtion.LocationDetail">
        select * from locationDetail where locationName=#{name}
    </select>
</mapper>